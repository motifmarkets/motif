{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Attach to Chrome",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///./*": "${webRoot}/*",
        "webpack:///./node_modules/@motifmarkets/motif-grid/node_modules/hypegrid/src/code/*": "${webRoot}/../../hypegrid/src/code/*",
        "webpack:///./node_modules/@motifmarkets/motif-grid/src/*": "${webRoot}/../../motif-grid/src/*",
        "webpack:///./node_modules/golden-layout/src/ts/*": "${webRoot}/../../golden-layout/src/ts/*",
      }
    },
  ]
}
