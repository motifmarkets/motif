{
  "name": "motif",
  "version": "2.7.6",
  "license": "SEE LICENSE IN license.txt",
  "homepage": "https://motifmarkets.com/platform/documentation/motif/",
  "author": {
    "name": "Motif Markets",
    "url": "https://www.motifmarkets.com/"
  },
  "bugs": {
    "url": "https://github.com/motifmarkets/motif/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "typings": "lib/public-api.d.ts",
  "scripts": {
    "postinstall": "generate-build-info.bat",
    "ng": "ng",
    "clean:cache": "npx del-cli .angular/cache",
    "clean:lib": "npx del-cli lib",
    "file:info:build": "generate-build-info.bat",
    "file:info:version": "type package.json | jq .version --raw-output > src/generated/version.txt",
    "file:info": "npm run file:info:build && npm run file:info:version",
    "copy:config:paritech_staging-dev": "copy src\\config\\config.paritech_staging-dev.json dev_static\\_config-do-not-delete\\config.json > $null && copy branding\\paritech\\paritech-desktop-bar-left-icon.svg dev_static\\_config-do-not-delete\\branding\\images\\desktop-bar-left-icon.svg > $null",
    "copy:config:trading-retailer_production-dev": "copy src\\config\\config.trading-retailer_production-dev.json dev_static\\_config-do-not-delete\\config.json > $null && copy branding\\paritech\\paritech-desktop-bar-left-icon.svg dev_static\\_config-do-not-delete\\branding\\images\\desktop-bar-left-icon.svg > $null",
    "copy:config:fnsx_demo-dev": "copy src\\config\\config.fnsx_demo-dev.json dev_static\\_config-do-not-delete\\config.json > $null",
    "copy:config:cfx_test-dev": "copy src\\config\\config.cfx_demo-dev.json dev_static\\_config-do-not-delete\\config.json > $null && copy branding\\cf-markets\\CF-Markets--Light-Logo.png dev_static\\_config-do-not-delete\\branding\\images\\CF-Markets--Light-Logo.png > $null",
    "copy:config:em2_cte-dev": "copy src\\config\\config.em2_cte-dev.json dev_static\\_config-do-not-delete\\config.json",
    "copy:config:em2_prod-dev": "copy src\\config\\config.em2_production-dev.json dev_static\\_config-do-not-delete\\config.json > $null",
    "copy:config:finplex_prod-dev": "copy src\\config\\config.finplex_production-dev.json dev_static\\_config-do-not-delete\\config.json > $null",
    "start:paritech_staging-dev": "npm run file:info && npm run copy:config:paritech_staging-dev && ng serve --host=127.0.0.1",
    "start:trading-retailer_production-dev": "npm run file:info && npm run copy:config:trading-retailer_production-dev && ng serve",
    "start:fnsx_demo-dev": "npm run file:info && npm run copy:config:fnsx_demo-dev && ng serve",
    "start:cfx_test-dev": "npm run file:info && npm run copy:config:cfx_test-dev && ng serve",
    "start:em2_cte-dev": "npm run file:info && npm run copy:config:em2_cte-dev && ng serve",
    "start:em2_prod-dev": "npm run file:info && npm run copy:config:em2_prod-dev && ng serve",
    "start:finplex_prod-dev": "npm run file:info && npm run copy:config:finplex_prod-dev && ng serve",
    "startSourceMaps": "ng serve --vendorSourceMap",
    "build:dev": "npm run file:info && ng build -c=development",
    "build:prod": "npm run file:info && node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build",
    "test": "ng test",
    "lint": "ng lint motif",
    "lib": "npm run clean:lib && npx tsc -p ./tsconfig.app.json --declaration --declarationMap --emitDeclarationOnly --declarationDir ./lib",
    "rollup": "npx api-extractor run --local --verbose",
    "api": "npm run lib && npm run rollup"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.2",
    "@angular/common": "^16.2.2",
    "@angular/compiler": "^16.2.2",
    "@angular/core": "^16.2.2",
    "@angular/forms": "^16.2.2",
    "@angular/platform-browser": "^16.2.2",
    "@angular/platform-browser-dynamic": "^16.2.2",
    "@angular/router": "^16.2.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@ctrl/tinycolor": "^3.5.0",
    "@jaames/iro": "^5.5.1",
    "@motifmarkets/motif-core": "^0.0.35",
    "@ng-select/ng-select": "^11.1.1",
    "@xilytix/revgrid": "^0.8.0",
    "angular-split": "^16.1.1",
    "angular-svg-icon": "^16.1.0",
    "codemirror": "^6.0.1",
    "decimal.js-light": "^2.5.1",
    "file-saver": "^2.0.5",
    "golden-layout": "^2.6.0",
    "nanoid": "^4.0.2",
    "oidc-client-ts": "^2.2.5",
    "rollbar": "^2.26.2",
    "rxjs": "^7.8.1",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.1",
    "@angular-eslint/builder": "^16.1.1",
    "@angular-eslint/eslint-plugin": "^16.1.1",
    "@angular-eslint/eslint-plugin-template": "^16.1.1",
    "@angular-eslint/schematics": "^16.1.1",
    "@angular-eslint/template-parser": "^16.1.1",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.2",
    "@angular/language-service": "^16.2.2",
    "@microsoft/api-extractor": "^7.36.4",
    "@types/jasmine": "^4.3.5",
    "@types/jasminewd2": "^2.0.9",
    "@types/webpack": "^5.28.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.48.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "install": "^0.13.0",
    "jasmine-core": "^5.0.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "sass": "^1.66.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2"
  }
}
