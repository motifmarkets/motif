{
  "name": "motif",
  "version": "2.0.4",
  "license": "SEE LICENSE IN license.txt",
  "homepage": "https://motionite.trade/motif",
  "author": {
    "name": "Motif Markets",
    "url": "https://www.motifmarkets.com/"
  },
  "bugs": {
    "url": "https://github.com/motifmarkets/motif/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "typings": "lib/public-api.d.ts",
  "scripts": {
    "postinstall": "generate-build-info.bat",
    "ng": "ng",
    "clean:lib": "npx del-cli lib",
    "file:info:build": "generate-build-info.bat",
    "file:info:version": "type package.json | jq .version --raw-output > src/generated/version.txt",
    "file:info": "npm run file:info:build && npm run file:info:version",
    "copy:config:paritech_staging-dev": "copy src\\config\\config.paritech_staging-dev.json dev_static\\_config-do-not-delete\\config.json > $null",
    "copy:config:fnsx_demo-dev": "copy src\\config\\config.fnsx_demo-dev.json dev_static\\_config-do-not-delete\\config.json > $null",
    "copy:config:em2_cte-dev": "copy src\\config\\config.em2_cte-dev.json dev_static\\_config-do-not-delete\\config.json",
    "copy:config:em2_prod-dev": "copy src\\config\\config.em2_production-dev.json dev_static\\_config-do-not-delete\\config.json > $null",
    "start:paritech_staging-dev": "npm run file:info && npm run copy:config:paritech_staging-dev && ng serve",
    "start:fnsx_demo-dev": "npm run file:info && npm run copy:config:fnsx_demo-dev && ng serve",
    "start:em2_cte-dev": "npm run file:info && npm run copy:config:em2_cte-dev && ng serve",
    "start:em2_prod-dev": "npm run file:info && npm run copy:config:em2_prod-dev && ng serve",
    "startSourceMaps": "ng serve --vendorSourceMap",
    "build:dev": "npm run file:info && ng build -c=development",
    "build:prod": "npm run file:info && node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build",
    "test": "ng test",
    "lint": "ng lint motif",
    "lib": "npm run clean:lib && npx tsc -p .\\tsconfig.app.json --declaration --declarationMap --emitDeclarationOnly --declarationDir ./lib",
    "rollup": "npx api-extractor run --local --verbose",
    "api": "npm run lib && npm run rollup"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.1",
    "@angular/common": "^12.0.1",
    "@angular/compiler": "^12.0.1",
    "@angular/core": "^12.0.1",
    "@angular/forms": "^12.0.1",
    "@angular/platform-browser": "^12.0.1",
    "@angular/platform-browser-dynamic": "^12.0.1",
    "@angular/router": "^12.0.1",
    "@ctrl/tinycolor": "^3.4.0",
    "@jaames/iro": "^5.5.1",
    "@motifmarkets/revgrid": "^1.0.3",
    "@ng-select/ng-select": "^7.1.0",
    "angular-split": "^5.0.0",
    "decimal.js-light": "^2.5.1",
    "file-saver": "^2.0.5",
    "fin-hypergrid": "^3.3.2",
    "golden-layout": "^2.2.0",
    "ngx-color-picker": "^11.0.0",
    "oidc-client": "^1.11.5",
    "rollbar": "^2.21.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "uuid": "^8.3.2",
    "webpack": "^5.37.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.1.0",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-devkit/build-angular": "^12.0.1",
    "@angular-eslint/builder": "12.2.0",
    "@angular-eslint/eslint-plugin": "12.2.0",
    "@angular-eslint/eslint-plugin-template": "12.2.0",
    "@angular-eslint/schematics": "12.2.0",
    "@angular-eslint/template-parser": "12.2.0",
    "@angular/cli": "^12.0.1",
    "@angular/compiler-cli": "^12.0.1",
    "@angular/language-service": "^12.0.1",
    "@microsoft/api-extractor": "^7.15.2",
    "@types/eslint": "^7.2.13",
    "@types/jasmine": "^3.7.4",
    "@types/jasminewd2": "^2.0.9",
    "@types/jquery": "^3.5.5",
    "@types/luxon": "^1.26.5",
    "@types/node": "^15.3.1",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsdoc": "^35.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "install": "^0.13.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "less": "^4.1.1",
    "ng-packagr": "^12.0.0",
    "protractor": "~7.0.0",
    "sass": "^1.33.0",
    "svg-sprite-loader": "^6.0.6",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
